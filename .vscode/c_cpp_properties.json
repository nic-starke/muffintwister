{
    "env": {
        "BUILD_DIR": "${workspaceRoot}/build"
    },
    "configurations": [
        {
            "name": "Midifighter",
            "customConfigurationVariables": {
                "cross_file": "avr/xmega/128a4u.cross",
                "platform": "midifighter"
            },
            "includePath": [
                "${workspaceFolder}/subprojects/lufa**",
                "${workspaceFolder}/subprojects/qcbor**",
                "/usr/avr/include/**"
            ],
            "defines": [
                "ARCH=ARCH_XMEGA",
                "ATXMEGA128A4U",
                "BOARD=USER_BOARD",
                "__AVR_XMEGA__",
                "USE_LUFA_CONFIG_HEADER"
            ],
            "cStandard": "c11",
            "intelliSenseMode": "${default}",
            "compilerArgs": [],
        },
        {
            "name": "Linux",
            "defines": [
            ],
            "cStandard": "c11",
            "intelliSenseMode": "${default}",
            "compilerArgs": [],
            "customConfigurationVariables": {
                "platform": "linux"
            }
        }
    ],
    "version": 4
}
